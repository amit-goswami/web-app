{
  "name": "project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run compile",
    "start": "next start",
    "test": "jest",
    "lint:check": "next lint",
    "lint-all": "npm run lint:check",
    "test:watch": "jest --watchAll",
    "prettier:check": "prettier --check \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "prettier:fix": "prettier --write \"**/*.{css,js,json,jsx,ts,tsx}\"",
    "prettier-all": "npm run prettier:check && npm run prettier:fix",
    "compile": "next build",
    "precompile": "npm run prettier-all && npm run lint-all"
  },
  "dependencies": {
    "@heroicons/react": "^2.1.1",
    "@tanstack/react-query": "^5.22.2",
    "axios": "^1.6.7",
    "joi": "^17.12.1",
    "next": "14.1.0",
    "re-resizable": "^6.9.11",
    "react": "^18",
    "react-dom": "^18",
    "react-hot-toast": "^2.4.1",
    "zustand": "^4.5.1"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.1.0",
    "postcss": "^8",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.3.0",
    "typescript": "^5"
  }
}
